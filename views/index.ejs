<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
   <%- include('./partials/boot.ejs') %>

  <body>
    <%- include('./partials/nav.ejs') %>

    <div id="Title">Welcome to Test Champ</div>
    <div class="container-fluid my-5">
      <div class="row">
        <div class="col-sm-9">
          <h3 class="text-center">JOBS</h3>
          <% job.slice(0,10).forEach(job=> { %>
            <a href="/job/jobs/<%= job._id%>" class="single text-decoration-none">
            <div class="card mb-3 shadow" style="max-width: 90%">
              <div class="row g-0">
                <div class="col-md-4">
                  <div id="jobImage" class="text-light h-100 d-flex justify-content-center align-items-center fw-bold display-5" style="background: linear-gradient(to bottom right, #0d6efd, indigo)"><%= job.jobImage %></div>
                </div>
                <div class="col-md-8 text-dark">
                  <div class="card-body">
                    <h5 class="card-title"><%= job.jobTitle %></h5>
                    <p class="card-text">
                      <%= job.jobSnippet %>
                    </p>
                    <p class="card-text">
                      <span class="">Last updated: <%= job.createdAt.toLocaleDateString() %> </span> |
                      Category: <strong><%= job.jobCategory %></strong>
                    </p>
                  </div>
                </div>
              </div>
            </div>
            </a>
            <% }) %>
        </div>
        <div class="col-sm-3">
          <h3 class="text-center">Mock Test</h3>
          <button class="btn btn-primary" style="width: fit-content">
            All mock tests >>
          </button>

            <div class="accordion" id="accordionExample">
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingOne">
                  <button
                    class="accordion-button"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target="#collapseOne"
                    aria-expanded="true"
                    aria-controls="collapseOne"
                  >
                    IBPS PO
                  </button>
                </h2>
                <div
                  id="collapseOne"
                  class="accordion-collapse collapse show"
                  aria-labelledby="headingOne"
                  data-bs-parent="#accordionExample"
                >
                  <div class="accordion-body">
                    <div class="btn-categories">
                      <a href="/ibpsrrb/rrbClerkPrelims2020">
                        <button class="btn-style">RRB Clerk Prelims 2020</button>
                      </a>
                      <a href="/ibpsrrb/rrbClerkPrelims2020">
                        <button class="btn-style">RRB Clerk Prelims 2020</button>
                      </a>
                      <a href="/ibpsrrb/rrbClerkPrelims2020">
                        <button class="btn-style">RRB Clerk Prelims 2020</button>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
              <div class="accordion-item">
                <h2 class="accordion-header" id="headingTwo">
                  <button
                    class="accordion-button collapsed"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target="#collapseTwo"
                    aria-expanded="false"
                    aria-controls="collapseTwo"
                  >
                    IBPS Clerical
                  </button>
                </h2>
                <div
                  id="collapseTwo"
                  class="accordion-collapse collapse"
                  aria-labelledby="headingTwo"
                  data-bs-parent="#accordionExample"
                >
                  <div class="accordion-body">
                    <div class="btn-categories">
                        <a href="/ibpsrrb/rrbClerkPrelims2020">
                          <button class="btn-style">RRB Clerk Prelims 2020</button>
                        </a>
                        <a href="/ibpsrrb/rrbClerkPrelims2020">
